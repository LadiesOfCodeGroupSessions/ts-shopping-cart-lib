{
  "name": "ts-shopping-cart-lib",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "lint:ts": "tslint src/**/*.ts && tslint src/main/*.ts",
    "build": "tsc --project tsconfig.json",
    "test": "jest",
    "start": "node index.ts"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.8",
    "babel-core": "6.26.3",
    "babel-jest": "24.9.0",
    "cross-env": "6.0.3",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.2",
    "ts-node": "8.10.2"
  },
  "type": "module",
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(\\.|/)(test)\\.(js|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "standard": {
    "env": [
      "node",
      "jest"
    ],
    "globals": [
      "expect"
    ]
  }
}
